---
import SvelteEmoji from "$emoji";
import type { EmojiCode } from "$lib/emojis";

type Props = {
	/**
	 * ||||||||||
	 * |--|--|--|--|--|--|--|--|--|
	 * | ![](data:image/webp;base64,) | ![](data:image/webp;base64,) | ![](data:image/webp;base64,) | ![](data:image/webp;base64,) | ![](data:image/webp;base64,) | ![](data:image/webp;base64,) | ![](data:image/webp;base64,) | ![](data:image/webp;base64,) | ![](data:image/webp;base64,) |
	 * | ahyj | blevota | bonk | clown | cool | eww | funny | giggity | gpl |
	 * | ![](data:image/webp;base64,) | ![](data:image/webp;base64,) | ![](data:image/webp;base64,) | ![](data:image/webp;base64,) | ![](data:image/webp;base64,) | ![](data:image/webp;base64,) | ![](data:image/webp;base64,) | ![](data:image/webp;base64,) | ![](data:image/webp;base64,) |
	 * | handsome | jojo | sad | shock | silly-ket | silly | smarty | tired | vietnam |
	 * | ![](data:image/webp;base64,) |
	 * | smile |
	 */
	icon: EmojiCode;
};

let { icon } = Astro.props;

if (!icon) {
	throw new Error('Emoji component requires a "icon" prop.');
}
---

<SvelteEmoji {icon} width={24} height={24} />
